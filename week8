#include<iostream> 
using namespace std; 
class student 
{ 
private: 
string name; 
int age; 
public: 
void setdata(string n,int a) 
{ 
name=n; 
age=a; 
} 
void display()
}; 
int main() 
cout<<"name:"<<name<<endl; 
cout<<"age:"<<age<<endl; 
cout<<”Roll No:24B11AI439”<<endl; 
student s1; 
s1.setdata("tony",20); 
s1.display(); 
student *ptr; 
ptr=&s1; 
ptr->display(); 
student *ptr2=new student; 
ptr2->setdata("bruce",22); 
ptr2->display(); 
delete ptr2; 
return 0; 
}
